{
	"name": "direct-kotlin",
	"displayName": "direct-kotlin",
	"description": "Direct Custom Kotlin extension",
	"version": "0.0.1",
	"publisher": "direct-team",
	"repository": "https://github.com/zachclarity/aidevsecops2024.git",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.directbuild",
				"title": "directbuild"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^8.26.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"source-map-support": "^0.5.12",
		"typescript": "^5.5.2"
	}
}
